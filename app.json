{
  "expo": {
    "name": "Spacebee",
    "slug": "Spacebee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Spacebee logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Spacebee logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.Spacebee",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/Spacebee logo.png",
        "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "b4377c17-b589-4c2f-9d28-49f8b2ebff05"
      }
    },
    "plugins": [
      "expo-font",
      "@react-native-community/datetimepicker"
    ]
  }
}
